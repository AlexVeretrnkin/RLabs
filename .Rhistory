# Зчитування файлу
data <- read_csv("search_engine_data.csv")
# Графік тренду використання пошукових двигунів
data_long <- tidyr::pivot_longer(data, cols = -Date, names_to = "SearchEngine", values_to = "Usage")
ggplot(data_long, aes(x = Date, y = Usage, color = SearchEngine)) +
geom_line() +
labs(title = "Використання пошукових двигунів", x = "Дата", y = "Відсоток") +
theme_minimal()
